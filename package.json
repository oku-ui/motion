{
  "name": "motion",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@9.12.2",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm turbo build",
    "build:skip": "pnpm nx run-many -t build --skip-nx-cache",
    "story": "pnpm storybook dev -p 6006 --no-open",
    "build:storybook": "pnpm storybook build",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:vue": "vitest --config ./packages/vue/vitest.config.ts",
    "prepare": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.1",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^8.4.2",
    "@storybook/vue3-vite": "^8.4.2",
    "@testing-library/dom": "^9.3.4",
    "@types/node": "^20.17.6",
    "@vitejs/plugin-vue": "^5.1.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.8.1",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.11.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.4.2",
    "storybook-dark-mode": "^4.0.2",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "unplugin-vue-macros": "^2.13.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.5.12"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm eslint . --fix"
  }
}
