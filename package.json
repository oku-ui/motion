{
  "name": "@oku-ui/motion",
  "type": "module",
  "version": "0.3.4",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "license": "MIT",
  "repository": "oku-ui/motion",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "pnpm turbo build",
    "build:skip": "pnpm nx run-many -t build --skip-nx-cache",
    "story": "pnpm storybook dev -p 6006 --no-open",
    "build:storybook": "pnpm storybook build",
    "release": "bumpp --commit --push --tag",
    "docs:build": "pnpm --filter docs docs:build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:vue": "vitest --config ./packages/vue/vitest.config.ts",
    "prepare": "npx simple-git-hooks",
    "bumpp": "bumpp package.json packages/*/package.json .docs/package.json",
    "pub:release": "cd packages/core && pnpm release",
    "pub:beta": "cd packages/core && pnpm release:beta",
    "pub:alpha": "cd packages/core && pnpm release:alpha"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.1",
    "@oku-ui/motion": "workspace:*",
    "@radix-ui/colors": "^3.0.0",
    "@storybook/addon-essentials": "^8.4.3",
    "@storybook/addon-interactions": "^8.4.3",
    "@storybook/addon-links": "^8.4.3",
    "@storybook/blocks": "^8.4.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^8.4.3",
    "@storybook/vue3-vite": "^8.4.3",
    "@testing-library/dom": "^9.3.4",
    "@types/node": "^20.17.6",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.8.1",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.11.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.4.3",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^3.4.14",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "unplugin-vue-macros": "^2.13.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.5.12"
  },
  "pnpm": {
    "overrides": {
      "@oku-ui/motion": "workspace:^"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm eslint . --fix"
  }
}
