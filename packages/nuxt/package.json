{
  "name": "@oku-ui/motion-nuxt",
  "type": "module",
  "version": "0.2.1",
  "description": "A tiny, performant animation library for NuxtJS",
  "author": "Mehmet productdevbook <hi@productdevbook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/oku-ui/motion",
  "repository": {
    "type": "git",
    "url": "https://github.com/oku-ui/motion.git"
  },
  "bugs": "https://github.com/oku-ui/motion/issues",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "motion",
    "motionone"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "nuxi prepare",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm prepack && pnpm bumpp --commit='version(nuxt): release %s' --no-tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.159",
    "@oku-ui/motion": "latest"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.14.159",
    "@nuxt/test-utils": "^3.14.4",
    "@types/node": "^20.17.6",
    "@vitejs/plugin-vue": "^5.1.5",
    "@vitest/coverage-v8": "^1.6.0",
    "bumpp": "^9.8.1",
    "happy-dom": "^12.10.3",
    "json-summary": "^1.3.0",
    "lint-staged": "^15.2.10",
    "playwright-core": "^1.48.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.11",
    "vitest": "^1.6.0",
    "vue": "^3.5.12"
  },
  "lint-staged": {
    "*": "pnpm eslint . --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
